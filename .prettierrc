{
    "quoteProps": "as-needed",
    "singleQuote": true,
    "arrowParens": "avoid",
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 120,
    "proseWrap": "always",
    "bracketSpacing": false,
    "trailingComma": "es5",
    "semi": true,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "ignore"
}
